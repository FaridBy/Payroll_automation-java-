<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.Tooltip?>
<AnchorPane prefHeight="400.0" prefWidth="751.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java__project.Controller">
   <children>
      <ImageView fx:id="srchview" fitHeight="199.0" fitWidth="240.0" layoutX="536.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@searchbutton.png" />
         </image>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <Button layoutX="49.0" layoutY="300.0" mnemonicParsing="false" onAction="#toadd" prefHeight="46.0" prefWidth="154.0" text="Add Employee">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="536.0" layoutY="300.0" mnemonicParsing="false" onAction="#tosearch" prefHeight="46.0" prefWidth="199.0" text="Search and add Amount">

         <font>
            <Font name="Berlin Sans FB Demi Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="26.0" layoutY="14.0" mnemonicParsing="false" onAction="#back1" prefHeight="27.0" prefWidth="67.0" text="Back" textFill="#230000">
