            <Font name="Book Antiqua Bold" size="13.0" />
         </font>
      </Button>
      <ImageView fx:id="addview" fitHeight="199.0" fitWidth="169.0" layoutX="54.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@addperson.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="listview" fitHeight="171.0" fitWidth="171.0" layoutX="293.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@liste.jpg" />
         </image>
      </ImageView>
      <Button layoutX="317.0" layoutY="300.0" mnemonicParsing="false" onAction="#tolist" prefHeight="46.0" prefWidth="123.0" text="Employee's List">
         <tooltip><Tooltip text="See your employee's list and their all amounts" /></tooltip>
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
